{
  "consumer": {
    "name": "amsterdam-app-frontend"
  },
  "provider": {
    "name": "amsterdam-app-backend"
  },
  "interactions": [
    {
      "description": "a GET request to retrieve city offices",
      "providerState": "provider allows getting city offices",
      "request": {
        "method": "GET",
        "path": "/api/v1/city/offices",
        "headers": {
          "Accept": "application/json; charset=utf-8"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "status": true,
          "result": {
            "offices": [
              {
                "identifier": "e9871a7716da02a4c20cfb06f9547005",
                "location": "Stadsloket Centrum",
                "url": "https://www.amsterdam.nl/contact/stadsloket-centrum/"
              }
            ]
          }
        },
        "matchingRules": {
          "$.headers.Content-Type": {
            "match": "regex",
            "regex": "application\\/json; *charset=utf-8"
          },
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}