{
  "files.associations": {
    "*.yml": "azure-pipelines"
  },
  "reduxdev.port": 8000,
  "reduxdev.hostname": "localhost",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.addMissingImports.ts": "explicit",
    "source.fixAll.eslint": "always"
  },
  "editor.formatOnSave": true,
  "react-ai-helper.aiBaseURL": "http://localhost:1337/v1",
  "react-ai-helper.aiModel": "codestral-22b",
  "react-ai-helper.testIDPrefixFileNameRegex": "src/modules/([^/]+)/",
  "cSpell.words": ["bottomsheet", "Linkings", "recyclepunten", "Sourceid"],
  "typescript.preferences.preferTypeOnlyAutoImports": true
}
