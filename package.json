{
    "name": "amsterdam-app",
    "version": "0.29.0",
    "private": true,
    "scripts": {
        "android:phone:dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
        "android:phone:prod": "react-native run-android --variant=prodDebug",
        "android:tablet": "emulator @Pixel_C_API_30 & react-native run-android --deviceId='Pixel_C_API_30' --variant=devDebug --appIdSuffix=dev",
        "ios:phone:prod": "react-native run-ios --simulator='iPhone 11'",
        "ios:phone:dev": "react-native run-ios --simulator='iPhone 11' --configuration='DevDebug' --scheme='AmsterdamApp Test'",
        "ios:tablet:prod": "react-native run-ios --simulator='iPad Pro (12.9-inch) (5th generation)'",
        "ios:tablet:dev": "react-native run-ios --simulator='iPad Pro (12.9-inch) (5th generation)' --configuration='DevDebug' --scheme='AmsterdamApp Test'",
        "start": "react-native start",
        "storybook": "start-storybook -p 7007",
        "storybook:build": "build-storybook",
        "test": "jest",
        "test-ci": "npx jest --ci --coverage",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "typescript": "npx tsc",
        "clean": "react-native-clean-project",
        "prepare": "husky install",
        "postinstall": "patch-package",
        "detox:build:ios": "detox build --configuration ios.sim.debug",
        "detox:test:ios": "detox test --configuration ios.sim.debug",
        "detox:test:cucumber:ios": "DETOX_CONFIGURATION=ios.sim.debug npx cucumber-js",
        "detox:build:android": "detox build --configuration android.emu.debug",
        "detox:test:android": "detox test --configuration android.emu.debug"
    },
